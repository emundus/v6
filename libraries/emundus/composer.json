{
    "name": "benj/emundus",
    "authors": [
        {
            "name": "Benjamin Rivalland",
            "email": "benjamin.rivalland@emundus.fr"
        }
    ],
    "config": {
        "platform-check": false,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://www.setasign.com/downloads/"
        }
    ],
    "require": {
        "phpoffice/phpspreadsheet": "1.26.0",
        "phpoffice/phpword": "1.2.0",
        "setasign/fpdf": "1.8.*",
        "setasign/fpdi": "^2.3",
        "tecnickcom/tcpdf": "6.3.*",
        "setasign/fpdi_pdf-parser": "^2.0",
        "psr/simple-cache": "^1.0",
        "svrnm/exceldatatables": "dev-master",
        "dompdf/dompdf": "^2.0",
        "guzzlehttp/psr7": "^2.5",
        "gotenberg/gotenberg-php": "^1.1",
        "php-http/guzzle7-adapter": "^1.0",
        "giggsey/libphonenumber-for-php": "^8.13"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.7"
    }
}
