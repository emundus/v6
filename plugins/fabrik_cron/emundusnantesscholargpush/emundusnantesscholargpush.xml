<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<extension
		group="fabrik_cron"
		method="upgrade"
		type="plugin"
		version="4.2.9">
	<name>eMundus - Nantes - Push files to ScholarG</name>
	<author>Hugo Moracchini</author>
	<creationDate>June 2020</creationDate>
	<copyright>Copyright (C) 2020 emundus.fr - All rights reserved.</copyright>
	<license>GNU/GPL http://www.gnu.org/copyleft/gpl.html</license>
	<authorEmail>dev@emundus.fr</authorEmail>
	<authorUrl>www.emundus.fr</authorUrl>
	<version>2.0.0</version>
	<description>A plugin used for pushing every complete file on Nantes each night to their ScholarG tool.</description>
	<files>
		<filename>emundusnantesscholargpush.xml</filename>
		<filename plugin="emundusnantesscholargpush">emundusnantesscholargpush.php</filename>
		<folder>forms</folder>
		<filename>index.html</filename>
		<folder>language</folder>
	</files>
	<languages>
		<language tag="en-GB">en-GB.plg_fabrik_cron_emundusnantesscholargpush.ini</language>
		<language tag="en-GB">en-GB.plg_fabrik_cron_emundusnantesscholargpush.sys.ini</language>
		<language tag="fr-FR">fr-FR.plg_fabrik_cron_emundusnantesscholargpush.ini</language>
		<language tag="fr-FR">fr-FR.plg_fabrik_cron_emundusnantesscholargpush.sys.ini</language>
	</languages>
</extension>
