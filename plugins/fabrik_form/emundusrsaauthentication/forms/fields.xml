                   label="PLG_FABRIK_FORM_EMUNDUSRSAAUTHENTICATION_PUBLIC_KEY_LABEL"/>

            <field name="emundusrsaauhtentication_public_key_content" type="textarea" default=""
                   description="PLG_FABRIK_FORM_EMUNDUSRSAAUTHENTICATION_PUBLIC_KEY_CONTENT_DESC"
                   label="PLG_FABRIK_FORM_EMUNDUSRSAAUTHENTICATION_PUBLIC_KEY_CONTENT_LABEL"/>

            <field name="emundusrsaauhtentication_jgroup" type="text" default="2"
                   label="PLG_FABRIK_FORM_EMUNDUSRSAAUTHENTICATION_JOOMLA_GROUP"/>

            <field name="emundusrsaauhtentication_eprofile" type="text" default="1000"
                   label="PLG_FABRIK_FORM_EMUNDUSRSAAUTHENTICATION_EMUNDUS_PROFILE"/>

            <field name="emundusrsaauhtentication_attributes_note" type="note" default=""
                   label="PLG_FABRIK_FORM_EMUNDUSRSAAUTHENTICATION_ATTRIBUTES_NOTE"/>

            <field name="emundusrsaauhtentication_attributes_id" type="text" default="id"
                   label="PLG_FABRIK_FORM_EMUNDUSRSAAUTHENTICATION_ATTRIBUTES_ID"/>

