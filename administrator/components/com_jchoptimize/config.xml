               description="COM_JCHOPTIMIZE_PRO_PHPANDEXTERNAL_DESC"
               default="0">
            <option value="0">JNO</option>
            <option value="1">JYES</option>
        </field>
        <field name="group_automatic_combine_end"
               type="jchgroup"
               section="end"
