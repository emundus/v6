    <fieldset name="basic" addfieldpath="/administrator/components/com_jchoptimize/fields"
              label="COM_JCHOPTIMIZE_BASIC_FIELDSET_LABEL"
    >
       <field name="ajax"
               type="ajax">
        </field>
