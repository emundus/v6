               default="0">
            <option value="0">JNO</option>
            <option value="1">JYES</option>
        </field>
        <field name="pro_smart_combine_values"
               type="hidden"
               default=""
        >
        </field>
        <field name="cache_lifetime"
               type="list"
               default="15"
               label="COM_JCHOPTIMIZE_CACHE_LIFETIME_LABEL"
               description="COM_JCHOPTIMIZE_CACHE_LIFETIME_DESCRIPTION">
            <option value="15">COM_JCHOPTIMIZE_15_MIN</option>
            <option value="30">COM_JCHOPTIMIZE_30_MIN</option>
            <option value="60">COM_JCHOPTIMIZE_1_HOUR</option>
            <option value="180">COM_JCHOPTIMIZE_3_HOURS</option>
            <option value="360">COM_JCHOPTIMIZE_6_HOURS</option>
            <option value="720">COM_JCHOPTIMIZE_12_HOURS</option>
            <option value="1440">COM_JCHOPTIMIZE_1_DAY</option>
            <option value="2880">COM_JCHOPTIMIZE_2_DAYS</option>
            <option value="10080">COM_JCHOPTIMIZE_7_DAYS</option>
            <option value="20160">COM_JCHOPTIMIZE_2_WEEKS</option>
        </field>
        <field name="html_minify_level"
               type="list"
               default="0"
               label="COM_JCHOPTIMIZE_MINIFY_LEVEL_LABEL"
               description="COM_JCHOPTIMIZE_MINIFY_LEVEL_DESCRIPTION">
            <option value="0">COM_JCHOPTIMIZE_MINIFY_LEVEL_BASIC</option>
            <option value="1">COM_JCHOPTIMIZE_MINIFY_LEVEL_ADVANCED</option>
            <option value="2">COM_JCHOPTIMIZE_MINIFY_LEVEL_ULTRA</option>
        </field>
        <field name="htaccess"
