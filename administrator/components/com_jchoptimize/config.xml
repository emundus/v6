               section="start"
               label="COM_JCHOPTIMIZE_GROUP_MISC_LABEL"
               description="COM_JCHOPTIMIZE_GROUP_MISC_DESC">
        </field>
        <field name="pro_downloadid"
               type="proonlytext"
               default=""
               label="COM_JCHOPTIMIZE_DOWNLOADID_LABEL"
               description="COM_JCHOPTIMIZE_DOWNLOADID_DESCRIPTION"
               size="50">
        </field>
        <!--<procode>-->
        <field name="hidden_api_secret"
               type="hidden"
               default="0aad0284"
        >
        </field>
        <!--</procode>-->
        <field name="cache_random_key"
               type="hidden"
               default=""
               >
        </field>
        <field name="debug"
               type="radio"
               class="btn-group"
               default="0"
               label="COM_JCHOPTIMIZE_DEBUG_LABEL"
               description="COM_JCHOPTIMIZE_DEBUG_DESCRIPTION">
            <option value="0">JNO</option>
            <option value="1">JYES</option>
        </field>
        <field name="group_basic_misc_end"
               type="jchgroup"
               section="end">
        </field>

