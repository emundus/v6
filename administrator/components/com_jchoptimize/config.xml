               class="btn-group"
               label="COM_JCHOPTIMIZE_TRY_CATCH_LABEL"
               description="COM_JCHOPTIMIZE_TRY_CATCH_DESCRIPTION"
               default="1"
               >
            <option value="0">JNO</option>
            <option value="1">JYES</option>
        </field>

        <field name="group_combine_files_end"
               type="jchgroup"
               section="end"
