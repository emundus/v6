    <!-- ##### Combine files -->
    <fieldset name="combine_files"
              label="COM_JCHOPTIMIZE_COMBINE_FILES_FIELDSET_LABEL">
        <!--
 Combine files
 -->
        <field name="group_combine_files_start"
               type="jchgroup"
               section="start"
               label="COM_JCHOPTIMIZE_GROUP_COMBINE_LABEL"
               description="COM_JCHOPTIMIZE_GROUP_COMBINE_DESC">
        </field>

