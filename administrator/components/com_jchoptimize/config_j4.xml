        </fieldset> -->
        <!--
        Javascript preserving execution order
        -->
        <fieldset name="group_exclude_js_peo"
                  label="COM_JCHOPTIMIZE_GROUP_EXCLUDE_JS_PEO_LABEL"
                  description="COM_JCHOPTIMIZE_GROUP_EXCLUDE_JS_PEO_DESC"
        >
            <field name="excludeJs_peo"
                   type="excludejs"
                   rows="3"
                   default=""
                   cols="20"
                   label="COM_JCHOPTIMIZE_EXCLUDE_JS_LABEL"
                   description="COM_JCHOPTIMIZE_EXCLUDE_JS_DESCRIPTION">
            </field>
            <field name="excludeJsComponents_peo"
                   type="excludejsextns"
                   rows="3"
                   default=""
                   cols="20"
                   label="COM_JCHOPTIMIZE_EXCLUDE_JS_COMPONENTS_LABEL"
                   description="COM_JCHOPTIMIZE_EXCLUDE_JS_COMPONENTS_DESCRIPTION">
            </field>
            <field name="excludeScripts_peo"
                   type="excludescripts"
                   rows="3"
                   default=""
                   cols="20"
                   label="COM_JCHOPTIMIZE_PRO_EXCLUDE_SCRIPT_LABEL"
                   description="COM_JCHOPTIMIZE_PRO_EXCLUDE_SCRIPT_DESC">
            </field>
            <field name="excludeAllScripts"
                   type="radio"
                   layout="joomla.form.field.radio.switcher"
                   label="COM_JCHOPTIMIZE_PRO_EXCLUDEALLSCRIPTS_LABEL"
                   description="COM_JCHOPTIMIZE_PRO_EXCLUDEALLSCRIPTS_DESC"
                   default="0">
                <option value="0">JNO</option>
                <option value="1">JYES</option>
            </field>
        </fieldset>
        <!--
        Javascript exclude ignoring execution order
        -->
        <fieldset name="group_exclude_ieo"
                  label="COM_JCHOPTIMIZE_GROUP_EXCLUDE_IEO_LABEL"
                  description="COM_JCHOPTIMIZE_GROUP_EXCLUDE_IEO_DESC"
        >
            <field name="excludeJs"
                   type="excludejs"
                   rows="3"
                   default=""
                   cols="20"
                   label="COM_JCHOPTIMIZE_EXCLUDE_JS_LABEL"
                   description="COM_JCHOPTIMIZE_EXCLUDE_JS_DESCRIPTION">
            </field>
            <field name="excludeJsComponents"
                   type="excludejsextns"
                   rows="3"
                   default=""
                   cols="20"
                   label="COM_JCHOPTIMIZE_EXCLUDE_JS_COMPONENTS_LABEL"
                   description="COM_JCHOPTIMIZE_EXCLUDE_JS_COMPONENTS_DESCRIPTION">
            </field>
            <field name="excludeScripts"
                   type="excludescripts"
                   rows="3"
                   default=""
                   cols="20"
                   label="COM_JCHOPTIMIZE_PRO_EXCLUDE_SCRIPT_LABEL"
                   description="COM_JCHOPTIMIZE_PRO_EXCLUDE_SCRIPT_DESC">
            </field>
        </fieldset>
        <!--
        Javascript Don't move to the bottom
        -->
        <fieldset name="group_dontmove"
                  label="COM_JCHOPTIMIZE_GROUP_DONTMOVE_LABEL"
                  description="COM_JCHOPTIMIZE_GROUP_DONTMOVE_DESC"
        >
            <field name="dontmoveJs"
                   type="excludejsurls"
                   rows="3"
                   default=""
                   cols="20"
                   label="COM_JCHOPTIMIZE_DONTMOVEJS_LABEL"
                   description="COM_JCHOPTIMIZE_DONTMOVEJS_DESC">
            </field>
            <field name="dontmoveScripts"
                   type="excludescripts"
                   rows="3"
                   default=""
                   cols="20"
                   label="COM_JCHOPTIMIZE_DONTMOVESCRIPTS_LABEL"
