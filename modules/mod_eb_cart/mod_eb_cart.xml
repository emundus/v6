<?xml version="1.0" encoding="utf-8"?>
<extension type="module" version="3.5" client="site" method="upgrade">
	<name>Events Booking - Cart Module</name>
	<author>Tuan Pham Ngoc</author>
	<creationDate>Jan 2010</creationDate>
	<copyright>Ossolution Team</copyright>
	<license>http://www.gnu.org/licenses/gpl-2.0.html GNU/GPL</license>
	<authorEmail>tuanpn@joomdonation.com</authorEmail>
	<authorUrl>www.joomdonation.com</authorUrl>
	<version>3.17.0</version>
	<description>This module is used to display list of events which registrants has added to cart</description>
	<files>
		<filename module="mod_eb_cart">mod_eb_cart.php</filename>		
		<filename module="mod_eb_cart">tmpl/default.php</filename>
	</files>	
	<config>
		<fields name="params">
			<fieldset name="basic">
				<field name="moduleclass_sfx" type="text"
					label="COM_MODULES_FIELD_MODULECLASS_SFX_LABEL"
					description="COM_MODULES_FIELD_MODULECLASS_SFX_DESC" />				
				<field name="item_id" type="text"
					label="Itemid" description="Itemid variable in the link in module">
				</field>	
			</fieldset>
		</fields>		
	</config>
</extension>
